{
  "name": "react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "echo \"React app running at: http://$(hostname -I | awk '{print $1}'):3000\" && next dev --turbopack --hostname 0.0.0.0",
    "dev:info": "echo 'Starts Next.js development server with Turbopack'",
    "build": "next build",
    "build:info": "echo 'Builds the application for production'",
    "lint": "next lint",
    "lint:info": "echo 'Lints the application'",
    "start": "next start",
    "start:info": "echo 'Starts the production server'"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^16.1.6",
    "eslint-config-next": "^16.1.6",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.1.6",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "typescript": "^5.9.3"
  }
}
