{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": false,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": ["dom", "es2015"],
    "noFallthroughCasesInSwitch": false,
    "skipLibCheck": false,
    "strict": false,
    "types": [
      "jest",
      "node",
    ],
  },
  "exclude": ["node_modules"],
  "extends": "../../tsconfig.json",
  "include": ["src"]
}
